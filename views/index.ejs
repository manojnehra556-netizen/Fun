<!DOCTYPE html>
<html>
<head>
    <title>किठाना न्यूज</title>
</head>
<body>

<h1>किठाना न्यूज</h1>

<% news.forEach(function(item){ %>
    <div style="border:1px solid black; padding:10px; margin:10px;">
        <h2><%= item.title %></h2>
        <p><%= item.content %></p>
        <small>Category: <%= item.category %></small>
    </div>
<% }) %>

</body>
</html>